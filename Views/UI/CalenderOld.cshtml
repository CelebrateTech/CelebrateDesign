<script>
    // Hidden Div Show js Start

    let openContentId = null;

    function OpenDiv(contentId) {
        const content = document.getElementById(contentId);
        const previousContent = openContentId ? document.getElementById(openContentId) : null;

        if (content.style.display === "none" || content.style.display === "") {
            // Show the new content
            content.style.display = "block";
            openContentId = contentId;
            // Hide the previously open content (if any)
            if (previousContent) {
                previousContent.style.display = "none";
            }
        } else {
            // Close the content
            content.style.display = "none";
            openContentId = null;
        }
    }

    // Hidden Div Show js End

</script>