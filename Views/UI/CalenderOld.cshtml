        color: rgb(var(--CrGy));
        border-radius: var(--Pl8);
        overflow: hidden;
    }

    .DePrMh, .DePrYr{
        display: grid;
        grid-template-columns: repeat(3, 1fr);
    }

        .DePrMh div {
            text-align: center;
            padding: var(--Pl25) 0;
            font-size: var(--Pl15);
           /*  height: var(--Pl20);
            width: var(--Pl73); */
