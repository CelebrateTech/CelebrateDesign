            display: grid;
            grid-template-columns: repeat(7, 1fr);   
            border-bottom: 1px solid #ccc;
            font-weight: 600;
        }

        .DePrDyHr div {
                text-align: center;
                padding: 8px 0;
            }

        .DePrDy ,.DePrDyWhPe{
            display: grid;
            grid-template-columns: repeat(7, 1fr);
        }

        .DePrDy div {
                text-align: center;
                padding: 10px 0;
                font-size: var(--Pl14);            
            }

/*De:date, Pr:picker, Dy:day, Wh:with, Pe:price*/
      
           .DePrDyWhPe div {
            text-align: center;
            padding: 2px 0;
            font-size: var(--Pl14);         
